        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".activity.MainActivity"
            android:theme="@style/Theme.Intro">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".activity.CadastroActivity" />
        <activity
            android:name=".activity.PrincipalActivity"
            android:label="@string/title_activity_principal"
            android:theme="@style/AppTheme" />
        <activity android:name=".model.Idade" />
        <activity android:name=".model.DataNascimento" />
        <activity android:name=".model.EstadoCivil" />
        <activity android:name=".model.Endereco" />
        <activity android:name=".model.Bairro" />
        <activity android:name=".model.Cidade" />
        <activity android:name=".model.CEP" />
        <activity android:name=".model.Telefone" />
        <activity android:name=".model.Celular" />
        <activity android:name=".model.Profissao" />
        <activity android:name=".model.Medicamentos" />
        <activity android:name=".model.Orteses"></activity>
    </application>

</manifest>